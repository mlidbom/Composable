<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <connectionStrings>
    <add name="EventStore" connectionString="Data Source=localhost;Initial Catalog=_Test_Composable_CQRS_EventSourcing_EventStore;User ID=_Test_Composable_CQRS_EventSourcing_EventStore;Password=_Test_Composable_CQRS_EventSourcing_EventStore" />
    <add name="KeyValueStore" connectionString="Data Source=localhost;Initial Catalog=_Test_Composable_CQRS_EventSourcing_DocumentDB;User ID=_Test_Composable_CQRS_EventSourcing_DocumentDB;Password=_Test_Composable_CQRS_EventSourcing_DocumentDB" />
  </connectionStrings>
  <log4net debug="false">
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="CQRS.Tests.log" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="2" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.XMLLayout" />
    </appender>
    <root>
      <level value="WARN" />
      <appender-ref ref="RollingFileAppender" />
    </root>
    <logger name="NHibernate">
      <level value="WARN" />
    </logger>
    <logger name="NServiceBus">
      <level value="INFO" />
    </logger>
    <logger name="Manpower">
      <level value="DEBUG" />
    </logger>
    <logger name="Composable">
      <level value="DEBUG" />
    </logger>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NServiceBus.Core" publicKeyToken="9fc386479f8a226c" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.6.0.1509" newVersion="2.6.0.1509" />
